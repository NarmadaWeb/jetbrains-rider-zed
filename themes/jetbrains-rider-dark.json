{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "JetBrains Rider Dark",
  "author": "RajaSunrise",
  "themes": [
    {
      "name": "JetBrains Rider Dark",
      "appearance": "dark",
      "style": {
        "background": "#2b2d30",
        "elevated_surface.background": "#2b2d30",
        "surface.background": "#2b2d30",
        "panel.background": "#2b2d30",
        "panel.focused_border": "#3474F0ff",
        "border": "#1E1F21ff",
        "border.variant": "#393B41ff",
        "border.focused": "#3474F0ff",
        "border.selected": "#3474F0ff",
        "border.transparent": "#00000000",
        "border.disabled": "#393B41ff",
        "element.background": "#2b2d30",
        "element.hover": "#2E436Bff",
        "element.active": "#0A3783ff",
        "element.selected": "#0A3783ff",
        "element.disabled": "#2b2d30",
        "drop_target.background": "#3474F050",
        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#2E436Bff",
        "ghost_element.active": "#0A3783ff",
        "text": "#BDBDBDff",
        "text.muted": "#9FA1A5ff",
        "text.placeholder": "#9FA1A580",
        "text.disabled": "#9FA1A590",
        "text.accent": "#82ACFCff",
        "icon": "#BDBDBDff",
        "icon.muted": "#9FA1A5ff",
        "icon.disabled": "#9FA1A590",
        "icon.placeholder": "#BDBDBD80",
        "icon.accent": "#82ACFCff",

        "status_bar.background": "#2b2d30",
        "title_bar.background": "#2b2d30",
        "toolbar.background": "#282424",
        "tab_bar.background": "#282424",
        "tab.inactive_background": "#282424",
        "tab.active_background": "#282424",
        "search.match_background": "#0A378380",

        "editor.background": "#262626ff",
        "editor.foreground": "#BDBDBDff",
        "editor.gutter.background": "#262626ff",
        "editor.subheader.background": "#2B2D30ff",
        "editor.active_line.background": "#2B2D30ff",
        "editor.highlighted_line.background": "#2E436B40",
        "editor.line_number": "#9FA1A5ff",
        "editor.active_line_number": "#BDBDBDff",
        "editor.invisible": "#BDBDBD50",
        "editor.wrap_guide": "#393B41ff",
        "editor.active_wrap_guide": "#82ACFCff",
        "editor.document_highlight.read_background": "#2E436B80",
        "editor.document_highlight.write_background": "#0A378380",

        "players": [
          { "cursor": "#3474F0ff", "selection": "#0A3783ff" },
          { "cursor": "#85C46Cff", "selection": "#85C46C40" },
          { "cursor": "#C8A16Dff", "selection": "#C8A16D40" },
          { "cursor": "#EC94C0ff", "selection": "#EC94C040" },
          { "cursor": "#C191FFff", "selection": "#C191FF40" },
          { "cursor": "#3ACC9Cff", "selection": "#3ACC9C40" },
          { "cursor": "#65C3CCff", "selection": "#65C3CC40" }
        ],

        "terminal.background": "#262626ff",
        "terminal.foreground": "#262626ff",
        "terminal.bright_foreground": "#ffffff",
        "terminal.ansi.black": "#262626ff",
        "terminal.ansi.bright_black": "#9FA1A5ff",
        "terminal.ansi.red": "#EC94C0ff",
        "terminal.ansi.bright_red": "#ff88c8ff",
        "terminal.ansi.green": "#85C46Cff",
        "terminal.ansi.bright_green": "#ADEB96ff",
        "terminal.ansi.yellow": "#C8A16Dff",
        "terminal.ansi.bright_yellow": "#ffca80ff",
        "terminal.ansi.blue": "#6C95EBff",
        "terminal.ansi.bright_blue": "#82ACFCff",
        "terminal.ansi.magenta": "#C191FFff",
        "terminal.ansi.bright_magenta": "#E2BFFFff",
        "terminal.ansi.cyan": "#65C3CCff",
        "terminal.ansi.bright_cyan": "#7fffff",
        "terminal.ansi.white": "#BDBDBDff",
        "terminal.ansi.bright_white": "#ffffffff",

        "syntax": {
          "comment": { "color": "#85C46Cff" },
          "keyword": { "color": "#6C95EBff" },
          "string": { "color": "#C8A16Dff" },
          "string.escape": { "color": "#BDBDBDff" },
          "number": { "color": "#EC94C0ff" },
          "boolean": { "color": "#6C95EBff" },
          "function": { "color": "#3ACC9Cff" },
          "type": { "color": "#C191FFff" },
          "property": { "color": "#65C3CCff" },
          "enum": { "color": "#E2BFFFff" },
          "constant": { "color": "#C191FFff" },
          "variable": { "color": "#BDBDBDff" },
          "variable.special": { "color": "#6C95EBff" },
          "operator": { "color": "#BDBDBDff" },
          "punctuation": { "color": "#BDBDBDff" },
          "tag": { "color": "#C191FFff" },
          "attribute": { "color": "#85C46Cff" },
          "constructor": { "color": "#C191FFff" },
          "emphasis": { "font_style": "italic" },
          "emphasis.strong": { "font_weight": 700 },
          "link_uri": { "color": "#82ACFCff", "font_style": "italic" },
          "link_text": { "color": "#82ACFCff", "font_style": "normal" },
          "title": { "color": "#C191FFff", "font_weight": 700 },
          "predictive": { "color": "#9FA1A5ff" }
        }
      }
    }
  ]
}
