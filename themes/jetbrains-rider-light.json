{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "JetBrains Rider Light (Enhanced)",
  "author": "Enhanced Version",
  "themes": [
    {
      "name": "JetBrains Rider Light (Enhanced)",
      "appearance": "light",
      "style": {
        "background": "#FCFCFCff",
        "elevated_surface.background": "#F5F7FAff",
        "surface.background": "#FFFFFFFF",
        "panel.background": "#F8FAFCff",
        "panel.focused_border": "#1A73E8ff",
        "border": "#D0D7DEff",
        "border.variant": "#C1C8D0ff",
        "border.focused": "#1A73E8ff",
        "border.selected": "#1A73E8ff",
        "border.transparent": "#00000000",
        "border.disabled": "#E4E7EBff",
        "element.background": "#EDF2F7ff",
        "element.hover": "#E2E8F0ff",
        "element.active": "#D4E3F8ff",
        "element.selected": "#D4E3F8ff",
        "element.disabled": "#F1F5F9ff",
        "drop_target.background": "#1A73E830",
        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#E2E8F0ff",
        "ghost_element.active": "#D4E3F8ff",
        "text": "#2D3748ff",
        "text.muted": "#718096ff",
        "text.placeholder": "#A0AEC0ff",
        "text.disabled": "#A0AEC0ff",
        "text.accent": "#1A73E8ff",
        "icon": "#4A5568ff",
        "icon.muted": "#718096ff",
        "icon.disabled": "#CBD5E0ff",
        "icon.placeholder": "#A0AEC0ff",
        "icon.accent": "#1A73E8ff",

        "status_bar.background": "#F5F7FAff",
        "title_bar.background": "#F5F7FAff",
        "toolbar.background": "#FFFFFFFF",
        "tab_bar.background": "#F5F7FAff",
        "tab.inactive_background": "#F5F7FAff",
        "tab.active_background": "#FFFFFFFF",
        "tab.active_underline": "#1A73E8ff",
        "search.match_background": "#FFEB3BAA",

        "editor.background": "#FFFFFFFF",
        "editor.foreground": "#2D3748ff",
        "editor.gutter.background": "#FCFCFCff",
        "editor.subheader.background": "#F5F7FAff",
        "editor.active_line.background": "#EBF4FF80",
        "editor.highlighted_line.background": "#E6F3FF80",
        "editor.line_number": "#A0AEC0ff",
        "editor.active_line_number": "#4A5568ff",
        "editor.invisible": "#D0D7DEff",
        "editor.wrap_guide": "#E2E8F0ff",
        "editor.active_wrap_guide": "#1A73E8ff",
        "editor.document_highlight.read_background": "#E6F3FF80",
        "editor.document_highlight.write_background": "#1A73E830",

        "players": [
          { "cursor": "#000000ff", "selection": "#ADD6FFff" },
          { "cursor": "#208C3Bff", "selection": "#CDE9D4ff" },
          { "cursor": "#B5651Dff", "selection": "#F2E2D4ff" },
          { "cursor": "#C70077ff", "selection": "#F5CCDEff" },
          { "cursor": "#58249Cff", "selection": "#DDD4E9ff" },
          { "cursor": "#007A58ff", "selection": "#CCE5DEff" },
          { "cursor": "#006D75ff", "selection": "#CCDFE1ff" }
        ],

        "terminal.background": "#FFFFFFFF",
        "terminal.foreground": "#2D3748ff",
        "terminal.bright_foreground": "#000000ff",
        "terminal.ansi.black": "#4A5568ff",
        "terminal.ansi.bright_black": "#718096ff",
        "terminal.ansi.red": "#E53E3Eff",
        "terminal.ansi.bright_red": "#F56565ff",
        "terminal.ansi.green": "#38A169ff",
        "terminal.ansi.bright_green": "#48BB78ff",
        "terminal.ansi.yellow": "#D69E2Eff",
        "terminal.ansi.bright_yellow": "#ECC94Bff",
        "terminal.ansi.blue": "#3182CEff",
        "terminal.ansi.bright_blue": "#4299E1ff",
        "terminal.ansi.magenta": "#9F7AEAff",
        "terminal.ansi.bright_magenta": "#B794F4ff",
        "terminal.ansi.cyan": "#00A6A6ff",
        "terminal.ansi.bright_cyan": "#0BC5EAff",
        "terminal.ansi.white": "#4A5568ff",
        "terminal.ansi.bright_white": "#1A202Cff",

        "syntax": {
          "comment": { "color": "#718096ff" },
          "keyword": { "color": "#3182CEff", "font_weight": 600 },
          "string": { "color": "#38A169ff" },
          "string.escape": { "color": "#2C5282ff" },
          "number": { "color": "#B83280ff" },
          "boolean": { "color": "#3182CEff", "font_weight": 600 },
          "function": { "color": "#3182CEff", "font_weight": 600 },
          "type": { "color": "#6B46C1ff", "font_weight": 600 },
          "property": { "color": "#2C5282ff" },
          "enum": { "color": "#6B46C1ff" },
          "constant": { "color": "#B83280ff" },
          "variable": { "color": "#2D3748ff" },
          "variable.special": { "color": "#3182CEff" },
          "operator": { "color": "#4A5568ff" },
          "punctuation": { "color": "#718096ff" },
          "tag": { "color": "#3182CEff" },
          "attribute": { "color": "#DD6B20ff" },
          "constructor": { "color": "#6B46C1ff" },
          "emphasis": { "font_style": "italic" },
          "emphasis.strong": { "font_weight": 700 },
          "link_uri": { "color": "#3182CEff", "font_style": "italic" },
          "link_text": { "color": "#3182CEff", "text_decoration": "underline" },
          "title": { "color": "#6B46C1ff", "font_weight": 700 },
          "predictive": { "color": "#A0AEC0ff" }
        }
      }
    }
  ]
}
